{
  "name": "vscode-interactive-tool",
  "displayName": "Interactive Tool",
  "description": "An interactive tool for VSCode",
  "version": "0.0.1",
  "publisher": "vs-code-tools",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:interactive-tool.showWebview"
  ],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "interactive-tool.showWebview",
        "title": "Show Interactive Tool",
        "icon": {
          "light": "static/icons/icon.svg",
          "dark": "static/icons/icon.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "interactive-tool-explorer",
          "title": "Interactive Tool",
          "icon": "static/icons/icon.svg"
        }
      ]
    },
    "views": {
      "interactive-tool-explorer": [
        {
          "type": "webview",
          "id": "interactive-tool.webview",
          "name": "Interactive Tool",
          "icon": "static/icons/icon.svg",
          "contextualTitle": "Interactive Tool"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "interactive-tool.showWebview",
          "when": "view == interactive-tool.webview",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest --coverage",
    "test:e2e": "jest test/e2e --coverage",
    "test:unit": "jest test/unit --coverage"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.80.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.27.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.21.0",
    "@babel/preset-typescript": "^7.21.0",
    "babel-jest": "^29.5.0"
  },
  "dependencies": {
    "vue": "^2.7.14",
    "vue-i18n": "^8.28.2"
  }
}